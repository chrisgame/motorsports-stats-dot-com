<h3>Fastest Laps</h3>
<table class="fastest-laps">
  <tr>
    <th>Position</th>
    <th>Car Number</th>
    <th>Driver</th>
    <th>Team</th>
    <th class="lap">Lap</th>
    <th class="timeOfDay">Time Of Day</th>
    <th class="averageSpeed">Average Speed</th>
    <th class="time">Time</th>
  </tr>
  {{#each model as |result|}}
    <tr>
      <td>{{result.position}}</td>
      <td>{{result.carNumber}}</td>
      <td>{{result.driver}}</td>
      <td>{{result.team}}</td>
      <td class="lap">{{result.lap}}</td>
      <td class="timeOfDay">{{result.timeOfDay}}</td>
      <td class="averageSpeed">{{result.averageSpeed}}</td>
      <td class="time">{{result.time}}</td>
    </tr>
  {{/each}}
</table>
