<h3>Best Sectors</h3>
<table class="best-sectors">
  <tr>
    <th>Sector</th>
    <th>Position</th>
    <th>Car Number</th>
    <th>Driver</th>
    <th class="time">Time</th>
  </tr>
  {{#each model as |sector|}}
    <tr>
      <td>{{sector.sector}}</td>
      <td>{{sector.position}}</td>
      <td>{{sector.carNumber}}</td>
      <td>{{sector.driver}}</td>
      <td class="time">{{sector.time}}</td>
    </tr>
  {{/each}}
</table>
