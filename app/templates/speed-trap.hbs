<h3>Speed Traps</h3>
<table class="speed-traps">
  <tr>
    <th>Position</th>
    <th>Car Number</th>
    <th>Driver</th>
    <th class="speed">Speed</th>
    <th class="timeOfDay">Time Of Day</th>
  </tr>
  {{#each model as |result|}}
    <tr>
      <td>{{result.position}}</td>
      <td>{{result.carNumber}}</td>
      <td>{{result.driver}}</td>
      <td class="speed">{{result.speed}}</td>
      <td class="timeOfDay">{{result.timeOfDay}}</td>
    </tr>
  {{/each}}
</table>
