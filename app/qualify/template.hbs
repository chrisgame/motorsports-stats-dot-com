<h3>Qualifying</h3>
<div local-class="qualifying-container">
  <ul local-class="qualifies">
  {{#each model.qualify_sessions as |qualify_session|}}
    <li local-class="qualify">{{#link-to 'qualify-session' qualify_session.id}} {{qualify_session.day}} {{qualify_session.session}} {{/link-to}}</li>
  {{/each}}
  </ul>
  {{outlet}}
</div>
