<table>
  <tr>
    <th>Position</th>
    <th>Car Number</th>
    <th>Driver</th>
    <th>Team</th>
    <th>Laps</th>
    <th>Time Or Retirement</th>
    <th>Grid</th>
    <th>Points</th>
  </tr>
  {{#each model as |result|}}
    <tr>
      <td>{{result.position}}</td>
      <td>{{result.carNumber}}</td>
      <td>{{result.driver}}</td>
      <td>{{result.team}}</td>
      <td>{{result.laps}}</td>
      <td>{{result.timeOrRetirement}}</td>
      <td>{{result.grid}}</td>
      <td>{{result.points}}</td>
    </tr>
  {{/each}}
</table>
